			_faultInCount = convertDBResult(resultInDirection,__time,__faultCount);
			_faultOutCount = convertDBResult(resultOutDirection,__time,__faultCount);
			if(_messageInCount){			
			len = _messageInCount.length-1;
			}
		
	};
	function formatTimeChunk(t){
		if (t<10){
  			t="0" + t;
		}
		return t;
	}
