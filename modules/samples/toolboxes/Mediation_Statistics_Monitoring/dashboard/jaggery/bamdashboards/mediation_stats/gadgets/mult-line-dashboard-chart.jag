		Math.max.apply(Math,faultInCount.map(function(o){return o[1];})),
		Math.max.apply(Math,faultOutCount.map(function(o){return o[1];})));

	if(isFinite(maxValue)){
		for(var i = 100; i < 1000000000 ; i){
			if(maxValue<i){
				var y = i/10;
				maxValue = Math.ceil(maxValue/y)*y;
				break;
			}
			i = i*10;
		}
		yMax = maxValue;
	}
	
	var lblXaxis = "<%=jqPlotXaxisLbl%>";

